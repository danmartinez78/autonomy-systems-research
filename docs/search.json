---
layout: null
---
[
  {% for page in site.pages %}
    {% unless page.url contains "/tags/" or page.url contains "404" %}
    {
      "title": {{ page.title | jsonify }},
      "url": {{ page.url | absolute_url | jsonify }},
      "summary": {{ page.summary | default: "" | jsonify }},
      "type": {{ page.type | default: "" | jsonify }},
      "tags": {{ page.tags | jsonify }},
      "content": {{ page.content | strip_html | strip_newlines | jsonify }},
      "date": {% if page.date %}"{{ page.date | date: "%Y-%m-%d" }}"{% elsif page.date_read %}"{{ page.date_read | date: "%Y-%m-%d" }}"{% elsif page.last_updated %}"{{ page.last_updated | date: "%Y-%m-%d" }}"{% else %}""{% endif %}
    }{% unless forloop.last %},{% endunless %}
    {% endunless %}
  {% endfor %}
]
